import jenkins.*
import jenkins.model.*
import hudson.*
import hudson.model.*

import hudson.scm.*
import hudson.tasks.*
import com.cloudbees.hudson.plugins.folder.*


node ()
{


    jen = Jenkins.instance

    jen.getItems().each{
        if(it instanceof Folder){
            processFolder(it)
        }else{
            processJob(it)
        }
    }

    void processJob(Item job){
      if (job.name ==~ /.*None$/){
        println job.name
      }
    }

    void processFolder(Item folder){
        folder.getItems().each{
            if(it instanceof Folder){
                processFolder(it)
            }else{
                processJob(it)
            }
        }
    }
    stage('Cleanup')
    {
        echo "Cleanup"
    }

}
